{
  "name": "@qhnu/ts-template",
  "version": "2.0.7",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog-ja"
    }
  },
  "dependencies": {
    "@types/jest": "^26.0.20",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog-ja": "^0.0.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "~24.3.1",
    "fixpack": "^4.0.0",
    "husky": "^5.1.3",
    "jest": "~26.6.3",
    "lint-staged": "^10.5.4",
    "next": "^10.0.8",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-eager": "^1.1.3",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@qhnu/config": "^2.0.5"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:dev": "ts-eager src/index.ts",
    "build:dist": "rimraf dist/* && tsc -p tsconfig.dist.json",
    "commit": "yarn run build:dist && git add -A && cz",
    "fixpack": "fixpack",
    "test:type": "tsc --noEmit",
    "test:watch": "jest --watchAll",
    "yarn:latest": "yarn upgrade --latest"
  }
}
